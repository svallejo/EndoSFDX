<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
        <fullName>CMPL123QMS__Investigation_Past_Due__c</fullName>
        <deprecated>false</deprecated>
        <description>This value checks on whether the Investigation phase of the CAPA is overdue</description>
        <externalId>false</externalId>
        <formula>(CMPL123QMS__Investigation_Due_Date__c  &lt;  CMPL123QMS__Investigation_Completed_Date__c) ||   ((ISBLANK( CMPL123QMS__Investigation_Completed_Date__c )  &amp;&amp; (Today() &gt;  CMPL123QMS__Investigation_Due_Date__c )))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Investigation Past Due</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </CustomField>
