<?xml version="1.0" encoding="utf-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
        <fullName>Prevent_Assessment_from_edit</fullName>
        <active>true</active>
        <description>Prevent Editing or adding assessment when the complaint is closed</description>
        <errorConditionFormula>CMPL123CME__Complaint__r.CMPL123CME__CMPL123_WF_Status__c =="Closed - Completed"</errorConditionFormula>
        <errorMessage>Lock Record</errorMessage>
    </ValidationRule>
