<?xml version="1.0" encoding="utf-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
        <fullName>CMPL123CME__B3_Date_of_Event_Cannot_be_Future_Date</fullName>
        <active>false</active>
        <description>Date cannot be greater than today's date</description>
        <errorConditionFormula>AND(RecordType.Name = "Initial",NOT(ISBLANK( CMPL123CME__B3_Date_of_Event__c )),CMPL123CME__B3_Date_of_Event__c &gt; TODAY())</errorConditionFormula>
        <errorDisplayField>CMPL123CME__B3_Date_of_Event__c</errorDisplayField>
        <errorMessage>Date cannot be greater than today's date</errorMessage>
    </ValidationRule>
