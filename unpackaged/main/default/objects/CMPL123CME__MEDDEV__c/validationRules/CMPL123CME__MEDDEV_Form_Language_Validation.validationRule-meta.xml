<?xml version="1.0" encoding="utf-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
        <fullName>CMPL123CME__MEDDEV_Form_Language_Validation</fullName>
        <active>true</active>
        <description>MEDDEV Form Language Validation, should accept only 'en', 'de' or blank values.</description>
        <errorConditionFormula>IF(OR(CMPL123CME__Form_Language__c = 'en',CMPL123CME__Form_Language__c = 'de',ISBLANK(CMPL123CME__Form_Language__c)),false,true)</errorConditionFormula>
        <errorDisplayField>CMPL123CME__Form_Language__c</errorDisplayField>
        <errorMessage>Permitted values: "en", "de", or blank.</errorMessage>
    </ValidationRule>
