<?xml version="1.0" encoding="utf-8"?><Workflow xmlns="http://soap.sforce.com/2006/04/metadata">
    <fieldUpdates>
        <fullName>Update_TR_Key_Value</fullName>
        <field>CMPL123Rule_Key_Value__c</field>
        <formula>"Training"</formula>
        <name>Update TR Key Value</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Formula</operation>
        <protected>false</protected>
        <reevaluateOnChange>true</reevaluateOnChange>
    </fieldUpdates>
    <fieldUpdates>
        <fullName>Update_Training_Record_Completed_DateTim</fullName>
        <field>CMPL123TMS__Completed_Date__c</field>
        <formula>CMPL123_WF_Action_Performed_On__c</formula>
        <name>Update Training Record Completed DateTim</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Formula</operation>
        <protected>false</protected>
        <reevaluateOnChange>true</reevaluateOnChange>
    </fieldUpdates>
    <fieldUpdates>
        <fullName>Update_Training_Record_Training_Status</fullName>
        <field>CMPL123TMS__Training_Status__c</field>
        <formula>CMPL123_WF_Status__c</formula>
        <name>Update Training Record Training Status</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Formula</operation>
        <protected>false</protected>
        <reevaluateOnChange>true</reevaluateOnChange>
    </fieldUpdates>
    <rules>
        <fullName>Assign Training Record Completed Date</fullName>
        <actions>
            <name>Update_Training_Record_Completed_DateTim</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>CMPL123TMS__Training_Status__c = "Completed" &amp;&amp; ISCHANGED(CMPL123_WF_Status__c) &amp;&amp; ISNULL(CMPL123TMS__Completed_Date__c)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Assign Training Record Key Value</fullName>
        <actions>
            <name>Update_TR_Key_Value</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>true</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Assign Training Record Status</fullName>
        <actions>
            <name>Update_Training_Record_Training_Status</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>AND(IF(CMPL123_WF_Status__c = CMPL123TMS__Training_Status__c , false,
true),!ISCHANGED(CMPL123TMS__Training_Status__c),CMPL123TMS__Training_Status__c
&lt;&gt; "Completed No Impact",
(IF(ISCHANGED(CMPL123_WF_Status__c) ||
ISBLANK(CMPL123TMS__Training_Status__c),true,false))
)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
</Workflow>
