<apex:page standardController="CMPL123CME__MDR__c" extensions="CMPL123CME.MdrReportController" standardStylesheets="false" showHeader="false" sidebar="false" renderAs="PDF"> 
    <html>
    <head>
    <style type="text/css">
        @page {   size: 8.5in 11in;/* width height */ }
        
        #main{
             width:100%;
        }
        #mainpg2{
            width:100%;
        }
        #mainpg3{
            width:100%;
        }
        
        #mainHeader2{
            margin: 20px;
        }
        #mainHeader3{
            margin: 20px;
        }
        #mainbody{
            margin: 20px;
            width:100%;
            float:left;
        }
        #mainbody2{
            margin: 20px;
            width:100%;
            float:left;
        }
        #mainbody3{
            margin: 20px;
            width:100%;
            float:left;
        }
        
        .leftmain{
            border-width:1px;
            border-style: solid ;
        }
        .rightmain{
            border-width:1px;
            border-style: solid ;
        }
        .secA{
            valign:top;
        }
        .secB{
            width:100;
        }
        
        .box{
            border-width: 1px;
            border-style: solid;
        }
    </style>
    </head>
    <body>
    <div id="main" style="font-size:8px;font-family: 'Arial Unicode MS';">
        <div id="mainHeader" style="width:100%; float:left;">
            <!--<div id="lefthead"  style="float:left; width:34%;">
                <b>U.S Department of Health and Human Services </b><br/>
                Food and Drug Adminstration
                <br/>
                <p style="font-size:22px"><b>MEDWATCH</b></p>
                <b>FORM FDA 3500A(2/13)</b>
            </div>-->
            <div id="lefthead"  style="float:left; width:34%;line-height:7pt"><apex:image url="{!URLFOR($Resource.CMPL123CME__MEDWatch_Header, '')}"/></div> 
            <div  id="middlehead" style="float:left; width:30%;line-height:7pt">
                <p style="text-align: center;">For use by user facilites,<br/>
                importers, distributors and manufactures<br/>
                for MANDATORY reporting<br/>
                <br/>
                <b>Page 1 of {!if(LEN(mdr.Describe_Event__c) < 3300 && LEN(mdr.Relevant_Tests_Lab_Data__c  ) < 1500 && LEN(mdr.Other_RelevantHistory__c  ) < 1500 && LEN(mdr.Concomitant_Med_Prod__c  )  < 350 && LEN(mdr.Concomitant_Medical_Prod__c  ) < 350 && LEN(mdr.Addtl_Mfg_Narrative__c) < 1800 && LEN(mdr.CorrectionData__c) < 1600, '2', '3')}</b>
                </p>
            </div>
            <div id="righthead" style="float:right; width:34%;line-height:7pt">
                <span style="text-align:right;">
                    <p style="text-align: right;">Form Approved: OMB No. 09 10-029 1, Expires 6/30/2015<br/>
                    see OMB statement on reverse.</p>
                </span>
                <table border="0" rules="rows" width="100%" cellspacing="0" cellpadding="0" style="border-top:1px solid #000000;border-left:1px solid #000000;border-right:1px solid #000000;border-bottom:1px solid #000000;">
                    <tr><td style="border-bottom:1px solid #000000;">
                        Mfr Report # &nbsp; {!if(LEN(mdr.Mfr_Report_Number__c) > 20,LEFT(mdr.Mfr_Report_Number__c,20), mdr.Mfr_Report_Number__c) }
                    </td></tr>
                    <tr><td style="border-bottom:1px solid #000000;">
                        UF/Importer Report # &nbsp; {!if(LEN(mdr.UF_Importer_Report_Number__c) > 20,LEFT(mdr.UF_Importer_Report_Number__c,20), mdr.UF_Importer_Report_Number__c) }
                    </td></tr>
                    <tr><td style="text-align: right;">
                        FDA Use Only
                    </td></tr>
                </table>
            </div>
        </div>
        <div id="mainbody">
            <div id="leftmain" class="leftmain" style="float:left; width:48%;">
                <div style="border-left:2px solid #000000;border-right:2px solid #000000;">
                <table style="width:100%" rules="cols" border="0" cellspacing="0" cellpadding="0">
                  <th style="background : #1C1C1C;color:#ffffff;text-align:left;font-size:10px;font-weight:bold" colspan="4">
                      A. PATIENT INFORMATION
                  </th>
                  <tr style="line-height:7pt;">
                      <td style="width:25%;height:50px;vertical-align:top;border-right:1px solid #000000;"><b>1. Patient Identifier</b><br/>
                          <!--<apex:outputText value="{!mdr.Complaint__r.Patient__r.CMPL123__Patient_ID__c}" />-->
                          <apex:outputText value="{!if(LEN(mdr.CMPL123CME__Patient_Identifier__c) > 10, LEFT(mdr.CMPL123CME__Patient_Identifier__c, 10), mdr.CMPL123CME__Patient_Identifier__c)}"></apex:outputText><br/><br/>
                          <br/><br/><span style="padding-left:20px">In Confidence</span>
                      </td>
                      <td  style="width:35%;height:50px;vertical-align:top;border-right:1px solid #000000;">
                          <b>2. Age at Time of Event:</b><br/> 
                          <!--<apex:outputText value="{!mdr.Complaint__r.Patient__r.CMPL123__Age_at_Time_of_Event__c}" />--> 
                          <apex:outputText value="{!mdr.CMPL123CME__Age_at_Time_of_Event__c}" />&nbsp;
                          <!--<apex:outputText value="{!mdr.Complaint__r.Patient__r.CMPL123__Age_Units__c}" />-->
                          <apex:outputText value="{!mdr.CMPL123CME__Age_Units_Patient__c}" /><br/> or <br/> 
                          <b>Date of Birth:</b> &nbsp;&nbsp;
                          <!--<apex:outputText value="{0,date,MM/dd/yyyy}">
                              <apex:param value="{!mdr.Complaint__r.Patient__r.CMPL123__Birthdate__c}" />
                          </apex:outputText>-->
                          <apex:outputText value="{0,date,MM/dd/yyyy}">
                              <apex:param value="{!mdr.CMPL123CME__DOB__c}" />
                          </apex:outputText>
                      </td>
                      <td style="width:20%;height:50px;vertical-align:top;border-right:1px solid #000000;"><b>3. Sex</b> <br/><br/>
                          <!--<apex:image width="10px" height="10px" url="{!IF((mdr.Complaint__r.Patient__r.CMPL123__Sex__c == 'Male'),'/resource/CMPL123CME__MDR/checkbox_yes.png', '/resource/CMPL123CME__MDR/checkbox_no.png')}"/> Male<br/>
                          <apex:image width="10px" height="10px" url="{!IF((mdr.Complaint__r.Patient__r.CMPL123__Sex__c == 'Female'),'/resource/CMPL123CME__MDR/checkbox_yes.png', '/resource/CMPL123CME__MDR/checkbox_no.png')}"/> Female</td>-->
                          &nbsp;&nbsp;&nbsp;<apex:image width="10px" height="10px" url="{!URLFOR($Resource.CMPL123CME__MDR, if(mdr.CMPL123CME__Sex__c == 'Female','/checkbox_yes.png','/checkbox_no.png'))}"/> Female<br/>
                          &nbsp;&nbsp;&nbsp;<apex:image width="10px" height="10px" url="{!URLFOR($Resource.CMPL123CME__MDR, if(mdr.CMPL123CME__Sex__c == 'Male','/checkbox_yes.png','/checkbox_no.png'))}"/> Male</td>
                      <td style="width:20%;height:50px;vertical-align:top;"><b>4. Weight</b><br/><br/>
                          <!--<apex:outputText value="{!mdr.Complaint__r.Patient__r.CMPL123__Weight__c}" />&nbsp;
                          <apex:outputText value="{!mdr.Complaint__r.Patient__r.CMPL123__Weight_Units__c}" />-->
                          
                          <table width="100%">
                              <tr style="line-height:7pt;">
                                  <td width="50%"><apex:outputText value="{!if(mdr.CMPL123CME__Weight_Units_Patient__c == 'LBS',mdr.CMPL123CME__Weight__c,'' )}" /></td>
                                  <td>lbs <br/> or</td>
                              </tr>
                              
                              <tr style="line-height:7pt;">
                                  <td><apex:outputText value="{!if(mdr.CMPL123CME__Weight_Units_Patient__c == 'KG',mdr.CMPL123CME__Weight__c,'' )}" /></td>
                                  <td>kgs</td>
                              </tr>
                          </table>
                          
                          
                          
                          
                          
                          <!--<apex:outputText value="{!if(mdr.Weight_Units_Patient__c == 'lbs',mdr.Weight__c,'' )}" />&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; lbs <br/> 
                          &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;or <br/>
                          <apex:outputText value="{!if(mdr.Weight_Units_Patient__c == 'kgs',mdr.Weight__c,'' )}" /> &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;kgs-->
                      </td>
                  </tr>
                </table>  
                <!------------- Section B ------------->
                <table class="secB" rules="rows" width="100%" cellspacing="0" cellpadding="0">
                  <th style="background : #1C1C1C;color:#ffffff;text-align:left;font-size:10px;font-weight:bold">
                      B. ADVERSE EVENT OR PRODUCT PROBLEM
                  </th>
                  <tr style="line-height:6pt;">
                      <td class="secA" style="width:100%;height:20px;border-bottom:1px solid #000000;">1. &nbsp;
                          <apex:image width="10px" height="10px" url="{!IF(CONTAINS(mdr.CMPL123CME__AdverseEvent_ProductProblem__c,'Adverse Event'),'/resource/CMPL123CME__MDR/checkbox_yes.png', '/resource/CMPL123CME__MDR/checkbox_no.png')}"/>&nbsp;<b>Adverse Event &nbsp;&nbsp;&nbsp; </b>and/or &nbsp;&nbsp;&nbsp;
                          <apex:image width="10px" height="10px" url="{!IF(CONTAINS(mdr.CMPL123CME__AdverseEvent_ProductProblem__c,'Product Problem(Eg.defects/malfunctions)'),'/resource/CMPL123CME__MDR/checkbox_yes.png', '/resource/CMPL123CME__MDR/checkbox_no.png')}"/>&nbsp;<b>Product Problem</b>(e.g., defects/malfunctions) 
                      </td>
                  </tr>
                  <tr style="line-height:7pt;">
                      <td  style="width:100%;height:80px;vertical-align:top;border-bottom:1px solid #000000;">2. <b>Outcomes Attributed to Adverse Event</b><br/>(Check all that apply)<br/>
                          <table cellspacing="0" cellpadding="0"> 
                              <tr style="line-height:7pt;">
                                  <td width="55%">&nbsp;&nbsp;&nbsp;<apex:image width="10px" height="10px" url="{!URLFOR($Resource.CMPL123CME__MDR, if(contains(mdr.CMPL123CME__Outcomes_attributed_to_AE__c,'Death'),'/checkbox_yes.png','/checkbox_no.png'))}"/>&nbsp;Death: &nbsp;&nbsp;<apex:outputText value="{0,date,MM/dd/yyyy}"><apex:param value="{!mdr.CMPL123CME__Dateof_Death__c}" /></apex:outputText></td> 
                                  <td><apex:image width="10px" height="10px" url="{!URLFOR($Resource.CMPL123CME__MDR, if(contains(mdr.CMPL123CME__Outcomes_attributed_to_AE__c,'Disability or Permanent Damage'),'/checkbox_yes.png','/checkbox_no.png'))}"/> Disability or Permanent Damage</td>
                              </tr>
                              <tr style="line-height:7pt;">                          
                                  <td>&nbsp;&nbsp;&nbsp;<apex:image width="10px" height="10px" url="{!URLFOR($Resource.CMPL123CME__MDR, if(contains(mdr.CMPL123CME__Outcomes_attributed_to_AE__c,'Life-threatening'),'/checkbox_yes.png','/checkbox_no.png'))}"/>&nbsp; Life-threatening</td>
                                  <td><apex:image width="10px" height="10px" url="{!URLFOR($Resource.CMPL123CME__MDR, if(contains(mdr.CMPL123CME__Outcomes_attributed_to_AE__c,'Congential Anomaly/Birth Defect'),'/checkbox_yes.png','/checkbox_no.png'))}"/> Congential Anomaly/Birth Defect</td>
                              </tr>
                              <tr style="line-height:7pt;">
                                  <td>&nbsp;&nbsp;&nbsp;<apex:image width="10px" height="10px" url="{!URLFOR($Resource.CMPL123CME__MDR, if(contains(mdr.CMPL123CME__Outcomes_attributed_to_AE__c,'Hospitalization'),'/checkbox_yes.png','/checkbox_no.png'))}"/>&nbsp; Hospitalization-initial or prolonged</td>
                                  <td><apex:image width="10px" height="10px" url="{!URLFOR($Resource.CMPL123CME__MDR, if(contains(mdr.CMPL123CME__Outcomes_attributed_to_AE__c,'Other Serious'),'/checkbox_yes.png','/checkbox_no.png'))}"/> Other Serious(Important Medical Events)</td>
                              </tr>
                              <tr style="line-height:7pt;">   
                                  <td colspan="2">&nbsp;&nbsp;&nbsp; <apex:image width="10px" height="10px" url="{!URLFOR($Resource.CMPL123CME__MDR, if(contains(mdr.CMPL123CME__Outcomes_attributed_to_AE__c,'Required Intervention'),'/checkbox_yes.png','/checkbox_no.png'))}"/> Required Intervention to Prevent Permanent Impairment/Damage(Devices)</td> 
                              </tr>
                          </table>
                      </td>
                  </tr>
                  <tr style="line-height:6pt;">
                      <td style="width:100%;border-bottom:1px solid #000000;">
                          <table cellspacing="0" cellpadding="0" width="100%">
                              <tr style="line-height:6pt;">
                                  <td style="width:50%;height:25px;vertical-align:top; border-right:1px solid #000000;">3. <b>Date of Event(mm/dd/yyyy) </b><br/><apex:outputText value="{0,date,MM/dd/yyyy}" ><apex:param value="{!mdr.CMPL123CME__Date_of_Event__c}" /></apex:outputText></td>
                                  <td style="width:50%;height:25px;vertical-align:top; ">4. <b>Date of This Report(mm/dd/yyyy) </b><br/><apex:outputText value="{0,date,MM/dd/yyyy}"><apex:param value="{!mdr.CMPL123CME__Date_of_this_Report__c}" /></apex:outputText></td>
                              </tr>
                          </table>
                      </td>
                      
                  </tr>
                   <tr style="line-height:6pt;">
                      <td style="width:100%;border-bottom:1px solid #000000;" >5. <b>Describe Event or Problem</b>
                          <div class="box" style="width:100%; height:300px">
                              <div style="word-wrap: break-word; width:330px"><apex:outputText value="{!if(LEN(mdr.CMPL123CME__Describe_Event__c) > 3000, LEFT(mdr.CMPL123CME__Describe_Event__c, 3000), mdr.CMPL123CME__Describe_Event__c)}" /></div>
                          </div>
                      </td>
                  </tr>
                   <tr style="line-height:6pt;">
                      <td style="width:100%;border-bottom:1px solid #000000;" >6. <b>Relavant Tests/Laboratory Data, Including Dates</b>
                          <div class="box" style="width:100%; height:150px">
                             <!--<apex:outputField value="{!mdr.Relevant_Tests_Lab_Data__c  }" style="height:150px;"/>-->
                             <apex:outputText style="height:150px;" value="{!if(LEN(mdr.CMPL123CME__Relevant_Tests_Lab_Data__c  ) > 1500, LEFT(mdr.CMPL123CME__Relevant_Tests_Lab_Data__c  , 1500), mdr.CMPL123CME__Relevant_Tests_Lab_Data__c  )}" />
                          </div>
                      </td>
                  </tr>
                   <tr style="line-height:6pt;">
                      <td style="border-bottom:2px solid #000000;">7. <b>Other Relevant History, Including Preexisting Medical Conditions (e.g., allergies, race,pregnacy, smoking and alcohol use, hepatic/renal dysfunction, etc. )</b>
                          <div class="box" style="width:100%; height:150px">
                              <apex:outputText value="{!if(LEN(mdr.CMPL123CME__Other_RelevantHistory__c ) > 1500, LEFT(mdr.CMPL123CME__Other_RelevantHistory__c  , 1500), mdr.CMPL123CME__Other_RelevantHistory__c  )}" />
                          </div>
                      </td>
                  </tr>
                </table>
                </div>
                <span style="line-height:5pt;"><b>Submission of a report does not constitute an addimission that medical personel, user facility, importer, distributor,manufacturer or product caused or contributed to the event.</b></span>
            </div>
            
            <div id="rightmain" class="rightmain" style="float:right; width:50%; margin-left:1% ;border-left:2px solid #000000;border-right:2px solid #000000; border-bottom:2px solid #000000;">
                <table rules="rows" width="100%" border="0" cellspacing="0" cellpadding="0">
                  <th style="background : #1C1C1C;color:#ffffff;text-align:left;font-size:10px;font-weight:bold">
                      C. SUSPECT PRODUCT(S)
                  </th>
                  <tr style="line-height:7pt;">
                      <td  style="width:100%;height:40px;vertical-align:top;border-bottom:1px solid #000000;">1. <b>Name</b>(Give labeled strength and mfr labeler)<br/>
                      #1&nbsp;&nbsp; <apex:outputText value="{!if(LEN(mdr.CMPL123CME__Medication_1__c) < 90, mdr.CMPL123CME__Medication_1__c, LEFT(mdr.CMPL123CME__Medication_1__c, 90))}" /> <!--  value="{!mdr.Medication_1__c}" -->
                      <br/><br/>
                      #2&nbsp;&nbsp; <apex:outputText value="{!if(LEN(mdr.CMPL123CME__Medication_2__c) < 90, mdr.CMPL123CME__Medication_2__c, LEFT(mdr.CMPL123CME__Medication_2__c, 90))}" /> <!-- value="{!mdr.Medication_2__c}"  -->
                      </td>
                  </tr>
                  <tr style="line-height:7pt;">
                      <td class="secA" style="width:100%;border-bottom:1px solid #000000;">
                          <table cellspacing="0" cellpadding="0" border="0" rules="cols">
                              <tr>
                                  <td style="width:50%;height:60px;vertical-align:top;border-right:1px solid #000000;"><b>2. Dose, Frequency and Route Used</b> <br/>
                                      #1&nbsp;&nbsp; <apex:outputText value="{!if(LEN(mdr.CMPL123CME__Dose_Freq_Route_1__c) < 90, mdr.CMPL123CME__Dose_Freq_Route_1__c, LEFT(mdr.CMPL123CME__Dose_Freq_Route_1__c, 90))}"/> <!-- value="{!mdr.Dose_Freq_Route_1__c}"  -->
                                      <hr size="1" width="90%"/>
                                      #2&nbsp;&nbsp; <apex:outputText value="{!if(LEN(mdr.CMPL123CME__Dose_Freq_Route_2__c) < 90, mdr.CMPL123CME__Dose_Freq_Route_2__c, LEFT(mdr.CMPL123CME__Dose_Freq_Route_2__c, 90))}" /> <!-- value="{!mdr.Dose_Freq_Route_2__c}" -->
                                  </td>
                                   <td style="width:50%;height:60px;vertical-align:top; "><b>3. Therapy Dates</b>(If Unknown, give duration) from/to (or best estimate) <br/>
                                      #1&nbsp;&nbsp; <apex:outputText value="{0,date,MM/dd/yyyy}" ><apex:param value="{!mdr.CMPL123CME__Therapy_Begin_date_1__c}" /></apex:outputText>&nbsp;&nbsp;<apex:outputText value="{0,date,MM/dd/yyyy}" ><apex:param value="{!mdr.CMPL123CME__Therapy_End_date_1__c}" /></apex:outputText>
                                      <hr size="1" width="90%"/>
                                      #2&nbsp;&nbsp; <apex:outputText value="{0,date,MM/dd/yyyy}" ><apex:param value="{!mdr.CMPL123CME__Therapy_Begin_date_2__c}" /></apex:outputText>&nbsp;&nbsp;<apex:outputText value="{0,date,MM/dd/yyyy}" ><apex:param value="{!mdr.CMPL123CME__Therapy_End_date_2__c}" /></apex:outputText>
                                  </td>
                              </tr>
                          </table>
                      </td>
                  </tr>
                  <tr>
                      <td class="secA" style="width:100%;border-bottom:1px solid #000000;">
                          <table width="100%" style="valign:top" cellspacing="0" cellpadding="0" border="0" rules="cols">
                              <tr style="line-height:7pt;">
                                  <td style="width:60%;valign:top;border-right:1px solid #000000;">
                                      <table width="100%" rules="rows" style="valign:top" cellspacing="0" cellpadding="0">
                                          <tr style="line-height:7pt;">
                                              <td style="height:40px;vertical-align:top;border-bottom:1px solid #000000;"><b>4. Diagnosis for Use(Indication)</b> <br/>
                                                  #1&nbsp;&nbsp; <apex:outputText value="{!if(LEN(mdr.CMPL123CME__Diagnosis_for_use_1__c) < 90, mdr.CMPL123CME__Diagnosis_for_use_1__c, LEFT(mdr.CMPL123CME__Diagnosis_for_use_1__c, 90))}" /> <!-- value="{!mdr.Diagnosis_for_use_1__c}" -->
                                                  <hr size="1" width="90%"/>
                                                  #2&nbsp;&nbsp; <apex:outputText value="{!if(LEN(mdr.CMPL123CME__Diagnosis_for_use_2__c) < 90, mdr.CMPL123CME__Diagnosis_for_use_2__c, LEFT(mdr.CMPL123CME__Diagnosis_for_use_2__c, 90))}"  /> <!--value="{!mdr.Diagnosis_for_use_2__c}" -->
                                              </td>
                                          </tr>
                                          <tr style="line-height:7pt;"><td style="width:100%;border-bottom:1px solid #000000;">
                                              <table style="width:100%" cellspacing="0" cellpadding="0">
                                                  <tr style="line-height:7pt;">
                                                      <td style="width:50%;height:40px;vertical-align:top; border-right:1px solid #000000;"><b>6. Lot #</b><br/>
                                                           #1&nbsp;&nbsp;<apex:outputText value="{!if(LEN(mdr.CMPL123CME__Lot_1__c) < 20, mdr.CMPL123CME__Lot_1__c, LEFT(mdr.CMPL123CME__Lot_1__c, 20))}" /> <!-- value="{!mdr.Lot_1__c}" -->
                                                          <hr size="1" width="90%"/>
                                                          #2&nbsp;&nbsp;<apex:outputText value="{!if(LEN(mdr.CMPL123CME__Lot_2__c) < 20, mdr.CMPL123CME__Lot_2__c, LEFT(mdr.CMPL123CME__Lot_2__c, 20))}" /> <!-- value="{!mdr.Lot_2__c}" -->
                                                      </td>
                                                      <td style="width:50%;height:40px;vertical-align:top;"><b>7. Exp. Date #</b><br/>
                                                           #1&nbsp;&nbsp;<apex:outputText value="{0,date,MM/dd/yyyy}" ><apex:param value="{!mdr.CMPL123CME__Exp_Date_1__c}" /></apex:outputText>
                                                          <hr size="1" width="90%"/>
                                                          #2 &nbsp;&nbsp;<apex:outputText value="{0,date,MM/dd/yyyy}" ><apex:param value="{!mdr.CMPL123CME__Exp_Date_2__c}"/></apex:outputText>
                                                      </td>
                                                  </tr>
                                              </table>
                                          </td></tr>
                                          <tr style="line-height:7pt;"><td style="height:30px;vertical-align:top;"><b>9. NDC # or Unique ID</b><br/><apex:outputText value="{!if(LEN(mdr.CMPL123CME__NDC_No__c) < 20, mdr.CMPL123CME__NDC_No__c, LEFT(mdr.CMPL123CME__NDC_No__c, 20))}" /></td></tr>  <!-- value="{!mdr.NDC_No__c}"-->
                                      </table>
                                  </td>
                                  <td class="SecA" style="width:40%">
                                      <table cellspacing="0" cellpadding="0">
                                          <tr style="line-height:7pt;"><td style="height:55px;vertical-align:top;border-bottom:1px solid #000000;">
                                              <b>5. Event Abated After Use Stopped or Dose Reduced ?</b><br/> 
