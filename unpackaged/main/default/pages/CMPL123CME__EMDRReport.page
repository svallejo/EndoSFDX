                    </apex:outputPanel>
                </div>
        </div>
        <datalist id="emdrfieldlist">
            <apex:repeat rendered="{!if(renderType != 'pdf',true,false)}" value="{!columns}" var="dt"> 
                <option value="{!dt}">{!dt}</option>
            </apex:repeat>
        </datalist>
        <script>
         window.onload = function(e){
            for(var ele in document.getElementsByTagName('input')){
                document.getElementsByTagName('input')[ele].setAttribute('list','emdrfieldlist');
            }
         }
        </script>
        
    </apex:form>
    </body>        
</apex:page>