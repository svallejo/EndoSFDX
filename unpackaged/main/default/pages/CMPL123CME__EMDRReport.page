<apex:page renderAs="{!renderType}" controller="CMPL123CME.DynamicReportController" cache="false" applyHtmlTag="false" applyBodyTag="false"  showHeader="false" sidebar="false" docType="html-5.0">
<apex:stylesheet value="{!$Resource.CMPL123CME__PDFDocumentStyle}" />
    <head>
        <style>
            body { font-family: 'Arial Unicode MS'; }
        </style>          
    </head>
    <body>
    <apex:form >
        <div style="font-family: 'Arial Unicode MS';text-align: center; display : {!if(renderType != 'pdf','block','none')}">
            <h1> EMDR Report Mapping </h1><br/><br/>
            <apex:commandButton action="{!doSave}" value="Save"/>
            <apex:commandButton rendered="{!if(isMDR == 'MDR',true,false)}" action="{!doLoad}" value="Load"/>
            <hr/>
            <br/><br/>
        </div>
        
        
        <c:PDFHeaderFooter type="header" position="center" >
              
                <b>
                    <div style="font-size:18px;font-family: 'Arial Unicode MS';">
                        <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['HDR1']}" html-placeholder="Select Column"  html-autocomplete="on" />
                        <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['HDR1']}"/>
                    </div>
                    <div style="font-size: 14px;font-family: 'Arial Unicode MS';">Medical Device Report Data</div>
                    <div style="font-size: 14px;font-family: 'Arial Unicode MS';">Submitted to CDRH as a Mandatory Report</div>
                    <div style="font-size: 14px;font-family: 'Arial Unicode MS';">in Electronic HL7 ICSR Format</div>
                    <div style="font-size: 14px;font-family: 'Arial Unicode MS';">Ref No: 
                        <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['HDR2']}" html-placeholder="Select Column"  html-autocomplete="on" />
                        <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['HDR2']}"/>
                    </div>
                    <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;display:{!if(renderType != 'pdf','none','block')};"><c:PDFHeaderFooter type="header" showPageNumbers="true"></c:PDFHeaderFooter></div>
                </b> 
                  
        </c:PDFHeaderFooter>
        <div style="{!if(renderType != 'pdf','margin-left:20px;width:90%;height:auto;border:1px solid black;','width:100%;')}">
            <div style="font-size:12px;font-family:'Arial Unicode MS',sans-serif;font-weight:bold;">A. PATIENT INFORMATION</div><br/>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;"><b>A1. Patient Identifier:</b>&nbsp; 
                <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['A1']}" html-placeholder="Select Column"  html-autocomplete="on" />
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['A1']}"/>
            </div>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;"><b>A2. Date of Birth:</b>&nbsp;  
                <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['A2']}" html-placeholder="Select Column"  html-autocomplete="on" />
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['A2']}"/>
            </div>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<b>Age at Time of Event:</b>&nbsp; 
                <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['A2.1']}"   html-placeholder="Select Column"  html-autocomplete="on" />
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['A2.1']}"/>
            </div>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;"><b>A3. Sex:</b>&nbsp; 
                <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['A3']}" html-placeholder="Select Column"  html-autocomplete="on" />
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['A3']}"/>
            </div>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;"><b>A4. Weight:</b>&nbsp; 
                <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['A4']}" html-placeholder="Select Column"  html-autocomplete="on" />
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['A4']}"/>
            </div> <br/>
            
            <!--------------------------------------  B Section Information ------------------------------------------->
            
            <div style="font-size:12px;font-family:'Arial Unicode MS',sans-serif;"><b>B. ADVERSE EVENT OR PRODUCT PROBLEM</b></div><br/>
            
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;"><b>B1. Adverse Event/Product Problem:</b>&nbsp;
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!IF(OR(CONTAINS(ResultMap['B1'],'Product Problem(Eg.defects/malfunctions)'),CONTAINS(ResultMap['B1'],'Adverse Event')),'',ResultMap['B1'])}"/>
                <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['B1']}" html-placeholder="Select Column"  html-autocomplete="on"/>
            </div>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;margin-left:18px;font-weight:bold;">
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!IF(CONTAINS(ResultMap['B1'],'Adverse Event'),'[X]','[]')}"/>&nbsp;Adverse Event
                <span style="margin-left:18px;">and/or &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!IF(CONTAINS(ResultMap['B1'],'Product Problem(Eg.defects/malfunctions)'),'[X]','[]')}"/>&nbsp;Product Problem</span>
            </div>
            
            
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;"><b>B2. Outcomes Attributed to Adverse Event:</b>&nbsp;
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!IF(OR(CONTAINS(ResultMap['B2'],'Death'),CONTAINS(ResultMap['B2'],'Life-threatening'),CONTAINS(ResultMap['B2'],'Hospitalization-initial or prolonged'),CONTAINS(ResultMap['B2'],'Disability or Permanent Damage'),CONTAINS(ResultMap['B2'],'Congential Anomaly/Birth Defect'),CONTAINS(ResultMap['B2'],'Required Intervention'),CONTAINS(ResultMap['B2'],'Other Serious(Important Medical Events)')),'',ResultMap['B2'])}"/>
                <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['B2']}" html-placeholder="Select Column"  html-autocomplete="on"/>
            </div> 
            
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;margin-left:18px;">
                <b><apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!IF(CONTAINS(ResultMap['B2'],'Death'),'[X]','[]')}"/>&nbsp;Death:</b>&nbsp;<apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['B2.1']}" html-placeholder="Select Column"  html-autocomplete="on"/> 
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!IF(CONTAINS(ResultMap['B2'],'Death'),ResultMap['B2.1'],'')}"/>
            </div>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;margin-left:18px;font-weight:bold;">
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!IF(CONTAINS(ResultMap['B2'],'Life-threatening'),'[X]','[]')}"/>&nbsp;Life-threatening
            </div>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;margin-left:18px;font-weight:bold;"> 
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!IF(CONTAINS(ResultMap['B2'],'Hospitalization'),'[X]','[]')}"/>&nbsp;Hospitalization-initial or prolonged
            </div>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;margin-left:18px;font-weight:bold;">
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!IF(CONTAINS(ResultMap['B2'],'Disability or Permanent Damage'),'[X]','[]')}"/>&nbsp;Disability or Permanent Damage
            </div>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;margin-left: 18px;font-weight:bold;">
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!IF(CONTAINS(ResultMap['B2'],'Congential Anomaly/Birth Defect'),'[X]','[]')}"/>&nbsp;Congential Anomaly/Birth Defect
            </div>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;margin-left:18px;font-weight:bold;"> 
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!IF(CONTAINS(ResultMap['B2'],'Required Intervention'),'[X]','[]')}"/>&nbsp;Required Intervention to Prevent Perm. Impairment/Damage (Devices)
            </div>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;margin-left:18px;">
                <b><apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!IF(CONTAINS(ResultMap['B2'],'Other Serious'),'[X]','[]')}"/>&nbsp;Other Serious(Important Medical Events):</b>
                &nbsp;<apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['B2_Other']}" html-placeholder="Select Column"  html-autocomplete="on"/>
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!IF(CONTAINS(ResultMap['B2'],'Other Serious'),ResultMap['B2_Other'],'')}"/>
            </div>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;"><b>B3. Date of Event:</b>&nbsp;
                <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['B3']}" html-placeholder="Select Column"  html-autocomplete="on"/>
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['B3']}"/>
            </div>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;"><b>B4. Date of This Report:</b>&nbsp;
                <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['B4']}" html-placeholder="Select Column"  html-autocomplete="on"/>
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['B4']}"/>
            </div>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;"><b>B5. Describe Event or Problem:</b>&nbsp;
                <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['B5']}" html-placeholder="Select Column"  html-autocomplete="on"/>
                <apex:outputPanel rendered="{!if(renderType != 'pdf',false,true)}">
                    <apex:outputText value="{!ResultMap['B5']}" style="white-space: pre-wrap;"/>
                </apex:outputPanel>
            </div>
            
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;"><b>B6. Relevant Tests/Laboratory Data, Including Dates:</b>&nbsp;
                <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['B6']}" html-placeholder="Select Column"  html-autocomplete="on"/>
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['B6']}" style="white-space: pre-wrap;"/>
            </div>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;"><b>B7. Other Relevant History, Including Preexisting Medical Conditions:</b>&nbsp;
                <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['B7']}" html-placeholder="Select Column"  html-autocomplete="on"/>
            </div>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;margin-left:22px;">
                <b>(e.g., allergies, race, pregnancy, smoking and alcohol use, hepatic/renal dysfunction, etc.)</b><br/>
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['B7']}"/>
            </div><br/>
            <!--<div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;"><b>B7. Other Relevant History, Including Preexisting Medical Conditions:</b>&nbsp;
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['B7']}"/>
            </div>--> 
            <!----------------------------------------------- C Section Information ------------------------------------------------------>
            
            <div style="font-size:12px;font-family:'Arial Unicode MS',sans-serif;font-weight:bold;">C. SUSPECT PRODUCTS - NOT APPLICABLE</div><br/>
            
            <!----------------------------------------------- D Section Information ------------------------------------------------------>
            
            <div style="font-size:12px;font-family:'Arial Unicode MS',sans-serif;font-weight:bold;">D. SUSPECT MEDICAL DEVICE</div><br/>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;"><b>D1. Brand Name:</b>&nbsp;
                <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['D1']}" html-placeholder="Select Column"  html-autocomplete="on"/>
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['D1']}"/>
            </div>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;"><b>D2. Common Device Name:</b>&nbsp;
                <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['D2']}" html-placeholder="Select Column"  html-autocomplete="on"/>
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['D2']}"/>
            </div>
            
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;margin-left:22px;"><b>D2b - Procode:</b>&nbsp;
                <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['D2.1']}" html-placeholder="Select Column"  html-autocomplete="on"/>
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['D2.1']}"/>
            </div>
            
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;"><b>D3. Manufacturer Name and Address:</b>&nbsp;
                <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['D3']}" html-placeholder="Select Column"  html-autocomplete="on"/>
                <!-- <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['D3']}"/> -->
            </div>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;margin-left:22px;"><b>Name:</b>&nbsp;
                <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['D3.1']}" html-placeholder="Select Column"  html-autocomplete="on"/>
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['D3.1']}"/>
            </div>            
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;margin-left:22px;"><b>Address 1:</b>&nbsp;
                <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['D3.2']}" html-placeholder="Select Column"  html-autocomplete="on"/>
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['D3.2']}"/>
            </div>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;margin-left:22px;"><b>Address 2:</b>&nbsp;
                <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['D3.3']}" html-placeholder="Select Column"  html-autocomplete="on"/>
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['D3.3']}"/>
            </div>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;margin-left:22px;"><b>City:</b>&nbsp;
                <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['D3.4']}" html-placeholder="Select Column"  html-autocomplete="on"/>
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['D3.4']}"/>
            </div>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;margin-left:22px;"><b>State:</b>&nbsp;
                <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['D3.5']}" html-placeholder="Select Column"  html-autocomplete="on"/>
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['D3.5']}"/>
            </div>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;margin-left:22px;"><b>Postal Code:</b>&nbsp;
                <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['D3.6']}" html-placeholder="Select Column"  html-autocomplete="on"/>
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['D3.6']}"/>
            </div>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;margin-left:22px;"><b>Country:</b>&nbsp;
                <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['D3.7']}" html-placeholder="Select Column"  html-autocomplete="on"/>
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['D3.7']}"/>
            </div>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;margin-left:22px;"><b>Email:</b>&nbsp;
                <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['D3.8']}" html-placeholder="Select Column"  html-autocomplete="on"/>
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['D3.8']}"/>
            </div>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;margin-left:22px;"><b>Fax:</b>&nbsp;
                <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['D3.9']}" html-placeholder="Select Column"  html-autocomplete="on"/>
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['D3.9']}"/>
            </div>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;font-weight:bold;">D4.</div>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;margin-left:22px;"><b>Catalog #:</b>&nbsp;
                <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['D4.1']}" html-placeholder="Select Column"  html-autocomplete="on"/>
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['D4.1']}"/>
            </div>            
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;margin-left:22px;"><b>Lot#:</b>&nbsp;
                <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['D4.2']}" html-placeholder="Select Column"  html-autocomplete="on"/>
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['D4.2']}"/>
            </div>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;margin-left:22px;"><b>Serial #:</b>&nbsp;
                <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['D4.3']}" html-placeholder="Select Column"  html-autocomplete="on"/>
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['D4.3']}"/>
            </div>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;margin-left:22px;"><b>Model #:</b>&nbsp;
                <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['D4.4']}" html-placeholder="Select Column"  html-autocomplete="on"/>
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['D4.4']}"/> 
            </div>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;margin-left:22px;"><b>Expiration Date:</b>&nbsp;
                <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['D4.5']}" html-placeholder="Select Column"  html-autocomplete="on"/>
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['D4.5']}"/>
            </div>
            
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;margin-left:22px;"><b>Other #:</b>&nbsp;
                <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['D4.6']}" html-placeholder="Select Column"  html-autocomplete="on"/>
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['D4.6']}"/>
            </div>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;margin-left:22px;"><b>Unique Identifier (UDI) #:</b>&nbsp;
                <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['D4.7']}" html-placeholder="Select Column"  html-autocomplete="on"/>
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!if(LEN(ResultMap['D4.7']) <= 100 , ResultMap['D4.7'], LEFT(ResultMap['D4.7'],100))}"/>
                <div>
                   <apex:outputText rendered="{!if(renderType == 'pdf' && LEN(ResultMap['D4.7']) > 100, true, false)}" value="{!RIGHT(ResultMap['D4.7'], LEN(ResultMap['D4.7'])-100)}"/>
                </div>
            </div>
            
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;"><b>D5. Operator of Device:</b>&nbsp;
                <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['D5']}" html-placeholder="Select Column"  html-autocomplete="on"/>
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['D5']}"/>
            </div>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;margin-left:22px;"><b>Other Text:</b>&nbsp;
                <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['D5.1']}" html-placeholder="Select Column"  html-autocomplete="on"/>
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['D5.1']}"/>
            </div>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;"><b>D6. Implant Date:</b>&nbsp;
                <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['D6']}" html-placeholder="Select Column"  html-autocomplete="on"/>
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['D6']}"/>
            </div>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;"><b>D7. Explant Date:</b>&nbsp;
                <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['D7']}" html-placeholder="Select Column"  html-autocomplete="on"/>
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['D7']}"/>
            </div>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;"><b>D8. Is this a Single-use Device that was Reprocessed and Reused on a Patient? </b>&nbsp;
                <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['D8']}" html-placeholder="Select Column"  html-autocomplete="on"/>
