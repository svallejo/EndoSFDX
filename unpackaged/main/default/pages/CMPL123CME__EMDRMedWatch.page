<apex:page standardController="CMPL123CME__EMDR__c" extensions="CMPL123CME.EMDRMedWatchController" standardStylesheets="false" showHeader="false" sidebar="false" renderAs="PDF" id="pg" applyHtmlTag="false" applyBodyTag="false"> 

<html>
    <head>
      <style type="text/css">                
          .f12SectionPadding {
            padding-left: 6px;
          }

          .leftPadding {
            padding-left: 1px;
          }
      </style>
    </head>
    <body>
        <div style="font-size:8px;">
            <div id="mainHeader" style="width:100%; float:left;">
            <div id="lefthead"  style="float:left; width:34%;"><apex:image url="{!URLFOR($Resource.CMPL123CME__MEDWatch_Header, '')}"/></div> 
            <div  id="middlehead" style="float:left; width:30%">
                <p style="text-align:center;">For use by user-facilites, importers,<br/>
                distributors and manufactures for<br/>
                MANDATORY REPORTING<br/>
                <br/>
                <b>Page 1 of {!if(LEN(mdr.B5_Describe_Event_or_Problem__c) < 2055 && LEN(mdr.B6_Relevant_Test_Laboratory_Data__c) < 990 && LEN(mdr.B7_Other_Relevant_History__c) < 990 && LEN(mdr.H10_Addtl_Mfg_Narrative__c) < 365 && LEN(mdr.CMPL123CME__H11_Corrected_Data__c) < 365, '2', '3')}</b>
                </p>
            </div>
            <div id="righthead" style="float:right; width:34%">
                <span style="text-align:right;">
                    <p style="text-align: right;"><b>Form Approved: OMB No. 0910-0291, Expires 9/30/2018</b><br/>
                    See PRA statement on reverse.</p>
                </span>
                <table border="0" rules="rows" width="100%" cellspacing="0" cellpadding="0" style="border-top:1px solid #000000;border-left:1px solid #000000;border-right:1px solid #000000;border-bottom:1px solid #000000;">
                    <tr><td style="border-bottom:1px solid #000000;">
                        Mfr Report # &nbsp; {!if(LEN(mdr.G9_MFR_Report_Number__c) > 20,LEFT(mdr.G9_MFR_Report_Number__c,20), mdr.G9_MFR_Report_Number__c) }
                    </td></tr>
                    <tr><td style="border-bottom:1px solid #000000;">
                        UF/Importer Report # &nbsp; {!if(LEN(mdr.F2_UF_Importer_Report_Number__c) > 20,LEFT(mdr.F2_UF_Importer_Report_Number__c,20), mdr.F2_UF_Importer_Report_Number__c) }
                    </td></tr>
                    <tr><td style="text-align: right;">
                        FDA Use Only
                    </td></tr>
                </table>
            </div>
        </div>
        <div>
            <div style="float:left; width:48%;">
                <div style="border-left:2px solid #000000;border-right:2px solid #000000;">
                <div style="border-top:2px solid #000000;"><i><b>&nbsp;Note:</b> For date prompts of “dd-mmm-yyyy” please use 2-digit day, 3-letter month abbreviation, and 4-digit year; for example, 01-Jul-2015.</i></div>
                <table style="width:100%" rules="cols" border="0" cellspacing="0" cellpadding="0">
                  <th style="background : #1C1C1C;color:#ffffff;text-align:left;font-size:10px;font-weight:bold" colspan="4">
                      A. PATIENT INFORMATION
                  </th>
                  <tr>
                      <td style="width:30%;height:30px;vertical-align:top;border-right:1px solid #000000;"><b>1. Patient Identifier</b><br/>
                          <apex:outputText value="{!if(LEN(mdr.CMPL123CME__A1_Patient_Identifier__c) > 10, LEFT(mdr.CMPL123CME__A1_Patient_Identifier__c, 10), mdr.CMPL123CME__A1_Patient_Identifier__c)}"></apex:outputText>
                          <br/><br/><span style="padding-left:20px"><i>In Confidence</i></span>
                      </td>
                      <td  style="width:40%;height:30px;vertical-align:top;border-right:1px solid #000000;">
                          <table width="100%" style="vertical-align:top;" cellpadding="0" cellspacing="0">
                              <tr>
                                  <td style="vertical-align:top;">
                                      <table width="100%" cellpadding="1" cellspacing="0">
                                          <tr>
                                              <td style="vertical-align:top;"><b>2. Age</b><br/>
                                                  &nbsp;&nbsp;&nbsp;<apex:outputText value="{!mdr.CMPL123CME__A2_Age_at_Time_of_Event__c}" />&nbsp;<br/>
                                                  &nbsp;&nbsp;&nbsp;<i>or</i>
                                              </td>
                                              <td>
                                                  &nbsp;<apex:image width="10px" height="10px" url="{!URLFOR($Resource.CMPL123CME__MDR, if(mdr.CMPL123CME__A2_Age_Units_Patient__c == 'Years','/checkbox_yes.png','/checkbox_no.png'))}"/>Year(s)&nbsp;<br/>
                                                  &nbsp;<apex:image width="10px" height="10px" url="{!URLFOR($Resource.CMPL123CME__MDR, if(mdr.CMPL123CME__A2_Age_Units_Patient__c == 'Months','/checkbox_yes.png','/checkbox_no.png'))}"/>Month(s)&nbsp;
                                              </td> 
                                              <td>
                                                  &nbsp;<apex:image width="10px" height="10px" url="{!URLFOR($Resource.CMPL123CME__MDR, if(mdr.CMPL123CME__A2_Age_Units_Patient__c == 'Weeks','/checkbox_yes.png','/checkbox_no.png'))}"/>Week(s)&nbsp;<br/>
                                                  &nbsp;<apex:image width="10px" height="10px" url="{!URLFOR($Resource.CMPL123CME__MDR, if(mdr.CMPL123CME__A2_Age_Units_Patient__c == 'Days','/checkbox_yes.png','/checkbox_no.png'))}"/>Day(s)&nbsp;
                                              </td>   
                                          </tr>
                                          <tr>
                                              <td colspan="3" style="vertical-align:top;">
                                                  &nbsp;&nbsp;&nbsp;<b>Date of Birth</b> <i>(e.g., 08-Feb-1925)</i><br/>
                                                  &nbsp;&nbsp;&nbsp;<apex:outputText value="{0,date,dd-MMM-yyyy}">
                                                      <apex:param value="{!mdr.CMPL123CME__A2_DOB__c}" />&nbsp;
                                                  </apex:outputText>
                                              </td>
                                          </tr>
                                      </table>
                                  </td>
                              </tr>
                          </table>
                      </td>
                      <td style="width:30%;height:30px;vertical-align:top;">
                          <table width="100%" cellpadding="1" cellspacing="0">
                              <tr>
                                  <td style="border-bottom:1px solid #000000;"><b>3. Sex</b><br/>
                                      &nbsp;&nbsp;&nbsp;<apex:image width="10px" height="10px" url="{!URLFOR($Resource.CMPL123CME__MDR, if(mdr.CMPL123CME__A3_Sex__c == 'Female','/checkbox_yes.png','/checkbox_no.png'))}"/> Female 
                                      &nbsp;&nbsp;&nbsp;<apex:image width="10px" height="10px" url="{!URLFOR($Resource.CMPL123CME__MDR, if(mdr.CMPL123CME__A3_Sex__c == 'Male','/checkbox_yes.png','/checkbox_no.png'))}"/> Male
                                  </td>
                              </tr>
                              <tr>
                                  <td>
                                      <table width="100%">
                                          <tr>
                                              <td colspan="3"><b>4. Weight</b></td>
                                          </tr>
                                          <tr>
                                              <td>{!mdr.A4_Weight__c}&nbsp;</td>
                                              <td>&nbsp;&nbsp;&nbsp;<apex:image width="10px" height="10px" url="{!URLFOR($Resource.CMPL123CME__MDR, if(mdr.CMPL123CME__A4_Weight_Units_Patient__c == 'LBS','/checkbox_yes.png','/checkbox_no.png'))}"/> lb</td>
                                              <td>&nbsp;&nbsp;&nbsp;<apex:image width="10px" height="10px" url="{!URLFOR($Resource.CMPL123CME__MDR, if(mdr.CMPL123CME__A4_Weight_Units_Patient__c == 'KG','/checkbox_yes.png','/checkbox_no.png'))}"/> kg</td>
                                          </tr>
                                      </table>
                                  </td>
                              </tr>
                          </table>
                      </td>
                  </tr>
                  <tr>
                      <td style="width:100%;border-top:1px solid #000000;" colspan="3">
                          <table width="100%" cellpadding="1" cellspacing="0">
                              <tr>
                                  <td style="width:35%;border-right:1px solid #000000;vertical-align:top;">
                                      <table width="100%" cellpadding="1" cellspacing="0">
                                          <tr><td>
                                              <b>5.a. Ethnicity</b><br/>&nbsp;&nbsp;&nbsp;&nbsp;<i>(Check single best answer)</i><br/>
                                              &nbsp;&nbsp;&nbsp;&nbsp;<apex:image width="10px" height="10px" url="{!URLFOR($Resource.CMPL123CME__MDR, if(JSENCODE(mdr.CMPL123CME__A5A_Ethnicity__c) == 'Hispanic/Latino','/checkbox_yes.png','/checkbox_no.png'))}"/> Hispanic/Latino<br/>
                                              &nbsp;&nbsp;&nbsp;&nbsp;<apex:image width="10px" height="10px" url="{!URLFOR($Resource.CMPL123CME__MDR, if(JSENCODE(mdr.CMPL123CME__A5A_Ethnicity__c) == 'Not Hispanic/Latino','/checkbox_yes.png','/checkbox_no.png'))}"/> Not Hispanic/Latino
                                          </td></tr>
                                      </table>
                                  </td>
                                  <td style="width:65%;vertical-align:top;">
                                      <table width="100%" cellpadding="1" cellspacing="0">
                                          <tr><td>
                                              <b>5.b. Race </b><i>(Check all that apply)</i><br/>
                                              &nbsp;&nbsp;&nbsp;&nbsp;<apex:image width="10px" height="10px" url="{!URLFOR($Resource.CMPL123CME__MDR, if(contains(JSENCODE(mdr.CMPL123CME__A5B_Race__c),'Asian'),'/checkbox_yes.png','/checkbox_no.png'))}"/> Asian 
                                              &nbsp;&nbsp;&nbsp;&nbsp;<apex:image width="10px" height="10px" url="{!URLFOR($Resource.CMPL123CME__MDR, if(contains(JSENCODE(mdr.CMPL123CME__A5B_Race__c),'American Indian or Alaskan Native'),'/checkbox_yes.png','/checkbox_no.png'))}"/> American Indian or Alaskan Native <br/>
                                              &nbsp;&nbsp;&nbsp;&nbsp;<apex:image width="10px" height="10px" url="{!URLFOR($Resource.CMPL123CME__MDR, if(contains(JSENCODE(mdr.CMPL123CME__A5B_Race__c),'Black or African American'),'/checkbox_yes.png','/checkbox_no.png'))}"/> Black or African American
                                              &nbsp;&nbsp;&nbsp;&nbsp;<apex:image width="10px" height="10px" url="{!URLFOR($Resource.CMPL123CME__MDR, if(contains(JSENCODE(mdr.CMPL123CME__A5B_Race__c),'White'),'/checkbox_yes.png','/checkbox_no.png'))}"/> White<br/>
                                              &nbsp;&nbsp;&nbsp;&nbsp;<apex:image width="10px" height="10px" url="{!URLFOR($Resource.CMPL123CME__MDR, if(contains(JSENCODE(mdr.CMPL123CME__A5B_Race__c),'Native Hawaiian or Other Pacific Islander'),'/checkbox_yes.png','/checkbox_no.png'))}"/> Native Hawaiian or Other Pacific Islander 
                                          </td></tr>
                                      </table>
                                  </td>
                              </tr>
                          </table>
                      </td>
                  </tr>
                </table>  
                <!------------- Section B ------------->
                <table rules="rows" width="100%" cellspacing="0" cellpadding="0">
                  <th style="background : #1C1C1C;color:#ffffff;text-align:left;font-size:10px;font-weight:bold">
                      B. ADVERSE EVENT OR PRODUCT PROBLEM
                  </th>
                  <tr>
                      <td style="width:100%;height:20px;border-bottom:1px solid #000000;">1. &nbsp;
                          <apex:image width="10px" height="10px" url="{!IF(CONTAINS(mdr.CMPL123CME__B1_AdverseEvent_ProductProblem__c,'Adverse Event') || CONTAINS(mdr.CMPL123CME__B1_AdverseEvent_ProductProblem__c,'Both'),'/resource/CMPL123CME__MDR/checkbox_yes.png', '/resource/CMPL123CME__MDR/checkbox_no.png')}"/>&nbsp;<b>Adverse Event &nbsp;&nbsp;&nbsp; </b>and/or &nbsp;&nbsp;&nbsp;
                          <apex:image width="10px" height="10px" url="{!IF(CONTAINS(mdr.CMPL123CME__B1_AdverseEvent_ProductProblem__c,'Product Problem')  || CONTAINS(mdr.CMPL123CME__B1_AdverseEvent_ProductProblem__c,'Both'),'/resource/CMPL123CME__MDR/checkbox_yes.png', '/resource/CMPL123CME__MDR/checkbox_no.png')}"/>&nbsp;<b>Product Problem</b>(e.g., defects/malfunctions) 
                      </td>
                  </tr>
                  <tr>
                      <td  style="width:100%;vertical-align:top;border-bottom:1px solid #000000;">2. <b>Outcomes Attributed to Adverse Event</b><i>(Check all that apply)</i><br/>
                          <table cellspacing="0" cellpadding="0"> 
                              <tr>
                                  <!-- Adding condition to display the date of death if B2 contains Death -->
                                  <td width="55%" colspan="2">&nbsp;&nbsp;&nbsp;<apex:image width="10px" height="10px" url="{!URLFOR($Resource.CMPL123CME__MDR, if(contains(mdr.CMPL123CME__B2_Outcomes_attributed_to_AE__c,'Death'),'/checkbox_yes.png','/checkbox_no.png'))}"/>&nbsp;Death-<i>Include date (dd-mmm-yyyy):</i>&nbsp;<apex:outputText value="{0,date,dd-MMM-yyyy}" rendered="{!if(contains(mdr.CMPL123CME__B2_Outcomes_attributed_to_AE__c,'Death'),true, false)}"><apex:param value="{!mdr.CMPL123CME__B2_Date_of_Death__c}" /></apex:outputText></td> 
                                  
                              </tr>
                              <tr>                          
                                  <td>&nbsp;&nbsp;&nbsp;<apex:image width="10px" height="10px" url="{!URLFOR($Resource.CMPL123CME__MDR, if(contains(mdr.CMPL123CME__B2_Outcomes_attributed_to_AE__c,'Life-threatening'),'/checkbox_yes.png','/checkbox_no.png'))}"/>&nbsp; Life-threatening</td>
                                  <td><apex:image width="10px" height="10px" url="{!URLFOR($Resource.CMPL123CME__MDR, if(contains(mdr.CMPL123CME__B2_Outcomes_attributed_to_AE__c,'Disability or Permanent Damage'),'/checkbox_yes.png','/checkbox_no.png'))}"/> Disability or Permanent Damage</td>
                              </tr>
                              <tr>
                                  <td>&nbsp;&nbsp;&nbsp;<apex:image width="10px" height="10px" url="{!URLFOR($Resource.CMPL123CME__MDR, if(contains(mdr.CMPL123CME__B2_Outcomes_attributed_to_AE__c,'Hospitalization-initial or prolonged'),'/checkbox_yes.png','/checkbox_no.png'))}"/>&nbsp; Hospitalization-initial or prolonged</td>
                                  <td><apex:image width="10px" height="10px" url="{!URLFOR($Resource.CMPL123CME__MDR, if(contains(JSENCODE(mdr.CMPL123CME__B2_Outcomes_attributed_to_AE__c),'Congenital Anomaly/Birth Defect'),'/checkbox_yes.png','/checkbox_no.png'))}"/> Congential Anomaly/Birth Defects {!CONTAINS(JSENCODE(mdr.B2_Outcomes_attributed_to_AE__c),'Congenital Anomaly/Birth Defect')}</td>
                              </tr>
                              <tr>
                                  <td colspan="2">&nbsp;&nbsp;&nbsp; <apex:image width="10px" height="10px" url="{!URLFOR($Resource.CMPL123CME__MDR, if(contains(JSENCODE(mdr.CMPL123CME__B2_Outcomes_attributed_to_AE__c),'Other Serious'),'/checkbox_yes.png','/checkbox_no.png'))}"/> Other Serious<i>(Important Medical Events)</i></td>
                              </tr>
                              <tr>   
                                  <td colspan="2">&nbsp;&nbsp;&nbsp; <apex:image width="10px" height="10px" url="{!URLFOR($Resource.CMPL123CME__MDR, if(contains(mdr.CMPL123CME__B2_Outcomes_attributed_to_AE__c,'Required Intervention'),'/checkbox_yes.png','/checkbox_no.png'))}"/> Required Intervention to Prevent Permanent Impairment/Damage<i>(Devices)</i></td> 
                              </tr>
                          </table>
                      </td>
                  </tr>
                  <tr>
                      <td style="width:100%;border-bottom:1px solid #000000;">
                          <table cellspacing="0" cellpadding="0" width="100%">
                              <tr>
                                  <td style="width:50%;height:25px;vertical-align:top; border-right:1px solid #000000;">3. <b>Date of Event</b><i>(dd-mmm-yyyy)</i> <br/><apex:outputText value="{0,date,dd-MMM-yyyy}" ><apex:param value="{!mdr.CMPL123CME__B3_Date_of_Event__c}" /></apex:outputText></td>
                                  <td style="width:50%;height:25px;vertical-align:top; ">4. <b>Date of This Report</b><i>(dd-mmm-yyyy)</i> <br/><apex:outputText value="{0,date,dd-MMM-yyyy}"><apex:param value="{!mdr.CMPL123CME__B4_Report_Date__c}" /></apex:outputText></td>
                              </tr>
                          </table>
                      </td>
                      
                  </tr>
                   <tr>
                      <td style="width:100%;border-bottom:1px solid #000000;" >5. <b>Describe Event or Problem</b>
                          <div style="width:100%; height:210px">
                              <div style="word-wrap: break-word; width:330px"><apex:outputText value="{!if(LEN(mdr.CMPL123CME__B5_Describe_Event_or_Problem__c) > 2055, LEFT(mdr.CMPL123CME__B5_Describe_Event_or_Problem__c, 2055), mdr.CMPL123CME__B5_Describe_Event_or_Problem__c)}" /></div>
                          </div>
                      </td>
                  </tr>
                   <tr>
                      <td style="width:100%;border-bottom:1px solid #000000;" >6. <b>Relevant Tests/Laboratory Data, Including Dates</b>
                          <div style="width:100%; height:100px">
                             <apex:outputText style="height:150px;" value="{!if(LEN(mdr.CMPL123CME__B6_Relevant_Test_Laboratory_Data__c  ) > 990, LEFT(mdr.CMPL123CME__B6_Relevant_Test_Laboratory_Data__c  , 990), mdr.CMPL123CME__B6_Relevant_Test_Laboratory_Data__c  )}" />
                          </div>
                      </td>
                  </tr>
                   <tr>
                      <td style="border-bottom:2px solid #000000;">7. <b>Other Relevant History, Including Preexisting Medical Conditions</b><i> (e.g., allergies, pregnancy, smoking and alcohol use, liver/kidney problems, etc.)</i>
                          <div style="width:100%; height:100px">
                              <apex:outputText value="{!if(LEN(mdr.CMPL123CME__B7_Other_Relevant_History__c ) > 990, LEFT(mdr.CMPL123CME__B7_Other_Relevant_History__c  , 990), mdr.CMPL123CME__B7_Other_Relevant_History__c  )}" />
                          </div>
                      </td>
                  </tr>
                </table>
                <table rules="rows" width="100%" border="0" cellspacing="0" cellpadding="0">
                    <!-- Modified C Section as per the form -->
                    <tr>
                        <th style="background : #1C1C1C;color:#ffffff;text-align:left;font-size:10px;font-weight:bold">
                            C. SUSPECT PRODUCT(S)
                        </th>
                    </tr>
                    <tr>
                        <td style="width:100%;border-bottom:2px solid #000000;">
                            <b>1. Name, Manufacturer/Compounder, Strength</b><br/>
                            <table width="100%" cellspacing="0" cellpadding="0">
                                <tr>
                                    <td style="vertical-align:top;"><b>#1</b></td>
                                    <td style="width:70%;vertical-align:top;">
                                        <b>Name and Strength</b><br/> <div style="height:18px;width:220px;">
                                        {!mdr.C1_Name_and_Strength1__c}
                                        </div>
                                    </td>
                                    <td style="vertical-align:top;">
                                        <b>NDC # or Unique ID</b><br/>
                                        <div style="height:18px;width:80px;">
                                            <apex:outputText value="{!LEFT(mdr.CMPL123CME__C1_NDC_or_Unique_ID1__c,25)}"/>
                                            <apex:outputPanel rendered="{!LEN(mdr.CMPL123CME__C1_NDC_or_Unique_ID1__c) > 25}">
                                                <div>
                                                    <apex:outputText value="{!IF(LEN(mdr.CMPL123CME__C1_NDC_or_Unique_ID1__c)>50, MID(mdr.CMPL123CME__C1_NDC_or_Unique_ID1__c,25,25), MID(mdr.CMPL123CME__C1_NDC_or_Unique_ID1__c,25,LEN(mdr.CMPL123CME__C1_NDC_or_Unique_ID1__c)-25))}"/>
                                                    <apex:outputPanel rendered="{!LEN(mdr.CMPL123CME__C1_NDC_or_Unique_ID1__c) > 50}">
                                                        <div>
                                                            <apex:outputText value="{!IF(LEN(mdr.CMPL123CME__C1_NDC_or_Unique_ID1__c)>75, MID(mdr.CMPL123CME__C1_NDC_or_Unique_ID1__c,50,25), MID(mdr.CMPL123CME__C1_NDC_or_Unique_ID1__c,50,LEN(mdr.CMPL123CME__C1_NDC_or_Unique_ID1__c)-50))}"/>
                                                            <apex:outputPanel rendered="{!LEN(mdr.CMPL123CME__C1_NDC_or_Unique_ID1__c)>75}">
                                                                <div>
                                                                    <apex:outputText value="{!MID(mdr.CMPL123CME__C1_NDC_or_Unique_ID1__c,75,LEN(mdr.CMPL123CME__C1_NDC_or_Unique_ID1__c)-75)}"/>
                                                                </div>   
                                                            </apex:outputPanel>
                                                        </div>
                                                    </apex:outputPanel>
                                                </div>
                                            </apex:outputPanel>
                                        </div>
                                    </td>
                                </tr>
                                <tr>
                                    <td colspan="2" style="vertical-align:top;width:70%;">
                                        <b>&nbsp;&nbsp;&nbsp;&nbsp;Manufacturer/Compounder</b><br/> 
                                        <div style="height:18px;width:220px;margin-left:7px;">{!mdr.C1_Manufacturer_Compounder1__c}</div>
                                    </td>
                                    <td style="vertical-align:top;">
                                        <b>Lot #</b><br/>
                                        <div style="height:18px;width:80px;">
                                            <apex:outputText value="{!LEFT(mdr.CMPL123CME__C1_Lot_Number1__c,25)}"/>
                                            <apex:outputPanel rendered="{!LEN(mdr.CMPL123CME__C1_Lot_Number1__c) > 25}">
                                                <div>
                                                    <apex:outputText value="{!IF(LEN(mdr.CMPL123CME__C1_Lot_Number1__c)>50, MID(mdr.CMPL123CME__C1_Lot_Number1__c,25,25), MID(mdr.CMPL123CME__C1_Lot_Number1__c,25,LEN(mdr.CMPL123CME__C1_Lot_Number1__c)-25))}"/>
                                                    <apex:outputPanel rendered="{!LEN(mdr.CMPL123CME__C1_Lot_Number1__c) > 50}">
                                                        <div>
                                                            <apex:outputText value="{!IF(LEN(mdr.CMPL123CME__C1_Lot_Number1__c)>75, MID(mdr.CMPL123CME__C1_Lot_Number1__c,50,25), MID(mdr.CMPL123CME__C1_Lot_Number1__c,50,LEN(mdr.CMPL123CME__C1_Lot_Number1__c)-50))}"/>
                                                            <apex:outputPanel rendered="{!LEN(mdr.CMPL123CME__C1_Lot_Number1__c)>75}">
                                                                <div>
                                                                    <apex:outputText value="{!MID(mdr.CMPL123CME__C1_Lot_Number1__c,75,LEN(mdr.CMPL123CME__C1_Lot_Number1__c)-75)}"/>
                                                                </div>   
                                                            </apex:outputPanel>
                                                        </div>
                                                    </apex:outputPanel>
                                                </div>
                                            </apex:outputPanel>
                                        </div>
                                    </td>
                                </tr>
                                <tr>
                                    <td style="vertical-align:top;"><b>#2</b></td>
                                    <td style="width:70%;vertical-align:top;">
                                        <b>Name and Strength</b><br/> <div style="height:18px;width:220px;">
                                        {!mdr.CMPL123CME__C1_Name_and_Strength2__c}
                                        </div>
                                    </td>
                                    <td style="vertical-align:top;">
                                        <b>NDC # or Unique ID</b><br/>
                                        <div style="height:18px;width:80px;">
                                            <apex:outputText value="{!LEFT(mdr.CMPL123CME__C1_NDC_or_Unique_ID2__c,25)}"/>
                                            <apex:outputPanel rendered="{!LEN(mdr.CMPL123CME__C1_NDC_or_Unique_ID2__c) > 25}">
                                                <div>
                                                    <apex:outputText value="{!IF(LEN(mdr.CMPL123CME__C1_NDC_or_Unique_ID2__c)>50, MID(mdr.CMPL123CME__C1_NDC_or_Unique_ID2__c,25,25), MID(mdr.CMPL123CME__C1_NDC_or_Unique_ID2__c,25,LEN(mdr.CMPL123CME__C1_NDC_or_Unique_ID2__c)-25))}"/>
