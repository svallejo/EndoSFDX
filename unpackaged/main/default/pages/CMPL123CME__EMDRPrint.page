<apex:page renderAs="{!renderType}" controller="CMPL123CME.EMDRPrintController" cache="false" applyHtmlTag="false" applyBodyTag="false"  showHeader="false" sidebar="false" docType="html-5.0">
<apex:stylesheet value="{!$Resource.CMPL123CME__PDFDocumentStyle}" />
    
    <body>
    <apex:form id="fm">
        <div style="font-family: 'Arial Unicode MS';text-align: center; display : {!if(renderType != 'pdf','block','none')}">
            <h1> EMDR Report Mapping </h1><br/><br/>
            <apex:commandButton action="{!doSave}" value="Save" reRender="fm"/>
            <apex:commandButton rendered="{!if(isMDR == 'MDR',true,false)}" action="{!doLoad}" value="Load" reRender="fm"/>
            <hr/>
            <br/><br/>
        </div>
        <c:PDFHeaderFooter type="header" position="center" rendered="{!if(renderType != 'pdf', false, true)}">
            <br/><div style="font-size: 24px;font-family:'sans-serif';text-align: center;">
               Medical Device Report Data 
            </div>
            <table style="width:100%;" align="center">
                <tr>
                    <td width="50%"><span style="font-size: 13px;font-weight: bold;font-family: 'sans-serif';">Printed from: </span><span style="font-size: 13px;font-family: 'sans-serif';">{!emdrName}</span></td>
                    <td width="50%"><span style="font-size: 13px;font-weight: bold;font-family: 'sans-serif';">Accepted by CDRH ?: </span><span style="font-size: 13px;font-family: 'sans-serif';">{!emdrSubmissionStatus}</span></td>
                </tr>
                <tr>
                    <td width="50%"><span style="font-size: 13px;font-weight: bold;font-family: 'sans-serif';">eMDR Type: </span><span style="font-size: 13px;font-family: 'sans-serif';">Supplemental</span></td>
                    <td width="50%"><span style="font-size: 13px;font-weight: bold;font-family: 'sans-serif';">Format: </span><span style="font-size: 13px;font-family: 'sans-serif';">HL7 ICSR</span></td>
                </tr>
                <tr>
                    <td width="50%"><span style="font-size: 13px;font-weight: bold;font-family: 'sans-serif';">Report Type: </span><span style="font-size: 13px;font-family: 'sans-serif';">Consolidated</span></td>
                    <td width="50%"><span style="font-size: 13px;font-weight: bold;font-family: 'sans-serif';">Reference No: </span><span style="font-size: 13px;font-family: 'sans-serif';">{!ResultMap['G9']}</span></td>
                </tr>
            </table>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;display:{!if(renderType != 'pdf','none','block')};"><c:PDFHeaderFooter type="header" showPageNumbers="true"></c:PDFHeaderFooter></div>
        </c:PDFHeaderFooter> 
        
        <c:PDFHeaderFooter type="header" position="center" rendered="{!if(renderType != 'pdf',true,false)}">
            <b>
                <div style="font-size:18px;font-family: 'Arial Unicode MS';">
                    <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['HDR1']}" html-placeholder="Select Column"  html-autocomplete="on" />
                    <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['HDR1']}"/>
                </div>
                <div style="font-size: 14px;font-family: 'Arial Unicode MS';">Medical Device Report Data</div>
                <div style="font-size: 14px;font-family: 'Arial Unicode MS';">Submitted to CDRH as a Mandatory Report</div>
                <div style="font-size: 14px;font-family: 'Arial Unicode MS';">in Electronic HL7 ICSR Format</div>
                <div style="font-size: 14px;font-family: 'Arial Unicode MS';">Ref No: 
                    <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['HDR2']}" html-placeholder="Select Column"  html-autocomplete="on" />
                    <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['HDR2']}"/>
                </div>
                <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;display:{!if(renderType != 'pdf','none','block')};"><c:PDFHeaderFooter type="header" showPageNumbers="true"></c:PDFHeaderFooter></div>
            </b> 
        </c:PDFHeaderFooter>
        <div style="{!if(renderType != 'pdf','margin-left:20px;width:90%;height:auto;border:1px solid black;','width:100%;')}"  class="contentBody">
            <div style="font-size:12px;font-family:'Arial Unicode MS',sans-serif;font-weight:bold;">A. PATIENT INFORMATION</div><br/>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;"><b>A1. Patient Identifier:</b> 
                <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['A1']}" html-placeholder="Select Column"  html-autocomplete="on" />
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['A1']}"/>
            </div>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;"><b>A2. Date of Birth:</b>  
                <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['A2']}" html-placeholder="Select Column"  html-autocomplete="on" />
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['A2']}"/>
            </div>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<b>Age at Time of Event:</b> 
                <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['A2.1']}"   html-placeholder="Select Column"  html-autocomplete="on" />
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['A2.1']}"/>
            </div>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;"><b>A3. Sex:</b> 
                <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['A3']}" html-placeholder="Select Column"  html-autocomplete="on" />
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['A3']}"/>
            </div>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;"><b>A4. Weight:</b> 
                <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['A4']}" html-placeholder="Select Column"  html-autocomplete="on" />
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['A4']}"/>
            </div>
            <apex:outputPanel rendered="{!if(emdrFormVersion == '2018', true, false)}">
                <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;"><b>A5. Ethnicity:</b> 
                    <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['A5.1']}" html-placeholder="Select Column"  html-autocomplete="on"/>
                    <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['A5.1']}"/> <!-- Added A5.1 to the consolidated print --><br/>
                </div>
                <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<b> Race:</b> 
                    <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['A5.2']}" html-placeholder="Select Column"  html-autocomplete="on"/>
                    <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['A5.2']}"/> <!-- Added A5.2 to the consolidated print -->
                </div>
            </apex:outputPanel><br/>
            
            <!--------------------------------------  B Section Information ------------------------------------------->
            
            <div style="font-size:12px;font-family:'Arial Unicode MS',sans-serif;"><b>B. ADVERSE EVENT OR PRODUCT PROBLEM</b></div><br/>
            
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;"><b>B1. Adverse Event/Product Problem:</b>&nbsp;
                <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['B1']}" html-placeholder="Select Column"  html-autocomplete="on"/>
            </div>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;margin-left:18px;font-weight:bold;">
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!IF(OR(CONTAINS(ResultMap['B1'],'Adverse Event'),CONTAINS(ResultMap['B1'],'Both')),'[X]','[]')}"/>&nbsp;Adverse Event
                <span style="margin-left:18px;">and/or &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!IF(OR(CONTAINS(ResultMap['B1'],'Product Problem'),CONTAINS(ResultMap['B1'],'Both')),'[X]','[]')}"/>&nbsp;Product Problem</span>
            </div>
            
            
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;"><b>B2. Outcomes Attributed to Adverse Event:</b>&nbsp;
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!IF(OR(CONTAINS(ResultMap['B2'],'Death'),CONTAINS(ResultMap['B2'],'Life-threatening'),CONTAINS(ResultMap['B2'],'Hospitalization-initial or prolonged'),CONTAINS(ResultMap['B2'],'Disability or Permanent Damage'),CONTAINS(ResultMap['B2'],'Congential Anomaly/Birth Defect'),CONTAINS(ResultMap['B2'],'Required Intervention'),CONTAINS(ResultMap['B2'],'Other Serious')),'',ResultMap['B2'])}"/>
                <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['B2']}" html-placeholder="Select Column"  html-autocomplete="on"/>
            </div> 
            
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;margin-left:18px;">
                <b><apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!IF(CONTAINS(ResultMap['B2'],'Death'),'[X]','[]')}"/>&nbsp;Death:</b>&nbsp;
                <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['B2.1']}" html-placeholder="Select Column"  html-autocomplete="on"/> 
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!IF(CONTAINS(ResultMap['B2'],'Death'),ResultMap['B2.1'],'')}"/>
            </div>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;margin-left:18px;font-weight:bold;">
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!IF(CONTAINS(ResultMap['B2'],'Life-threatening'),'[X]','[]')}"/>&nbsp;Life-threatening
            </div>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;margin-left:18px;font-weight:bold;"> 
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!IF(CONTAINS(ResultMap['B2'],'Hospitalization-initial or prolonged'),'[X]','[]')}"/>&nbsp;Hospitalization-initial or prolonged
            </div>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;margin-left:18px;font-weight:bold;">
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!IF(CONTAINS(ResultMap['B2'],'Disability or Permanent Damage'),'[X]','[]')}"/>&nbsp;Disability or Permanent Damage
            </div>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;margin-left: 18px;font-weight:bold;">
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!IF(CONTAINS(ResultMap['B2'],'Congenital Anomaly/Birth Defect'),'[X]','[]')}"/>&nbsp;Congential Anomaly/Birth Defect
            </div>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;margin-left:18px;font-weight:bold;"> 
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!IF(CONTAINS(ResultMap['B2'],'Required Intervention'),'[X]','[]')}"/>&nbsp;Required Intervention to Prevent Perm. Impairment/Damage (Devices)
            </div>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;margin-left:18px;font-weight:bold;">
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!IF(CONTAINS(ResultMap['B2'],'Other Serious'),'[X]','[]')}"/>&nbsp;Other Serious(Important Medical Events)
            </div>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;"><b>B3. Date of Event:</b>&nbsp;
                <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['B3']}" html-placeholder="Select Column"  html-autocomplete="on"/>
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['B3']}"/>
            </div>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;"><b>B4. Date of This Report:</b>&nbsp;
                <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['B4']}" html-placeholder="Select Column"  html-autocomplete="on"/>
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['B4']}"/>
            </div>
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;"><b>B5. Describe Event or Problem:</b>&nbsp;
                <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['B5']}" html-placeholder="Select Column"  html-autocomplete="on"/>
            </div>
            <div class="fieldContainer">
                <apex:outputPanel rendered="{!if(renderType != 'pdf',false,true)}">
                    <apex:outputText value="{!ResultMap['B5']}" style="white-space: pre-wrap;"/>
                </apex:outputPanel>
            </div>
            
            <div  class="fieldLabelMargin" style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;"><b>B6. Relevant Tests/Laboratory Data, Including Dates:</b>&nbsp;
                <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['B6']}" html-placeholder="Select Column"  html-autocomplete="on"/>
            </div>
            <div class="fieldContainer">
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['B6']}" style="white-space: pre-wrap;"/>
            </div>
            <div class="fieldLabelMargin" style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;"><b>B7. Other Relevant History, Including Preexisting Medical Conditions:</b>&nbsp;
                <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['B7']}" html-placeholder="Select Column"  html-autocomplete="on"/>
            </div>
            <div class="fieldContainer">
               <b> (e.g., allergies, race, pregnancy, smoking and alcohol use, hepatic/renal dysfunction, etc.) </b> <br/>  
                <apex:outputText styleClass="wrapWhiteSpace" rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['B7']}"/>
            </div><br/>
            <!--<div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;">&nbsp;
                <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['B7']}"/>
            </div>--> 
            <!----------------------------------------------- C Section Information ------------------------------------------------------>
            
            <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;font-weight:bold;"><apex:outputText value="{!if(emdrFormVersion == '2018', 'C. SUSPECT PRODUCTS','C. SUSPECT PRODUCTS - NOT APPLICABLE')}"></apex:outputText></div><br/>

            <apex:outputPanel rendered="{!if(emdrFormVersion == '2018', true, false)}">
                <div class="fieldLabelMargin" style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;font-weight:bold;"><b>C1. #1 Name and Strength:</b>&nbsp;
                    <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['C1.1.1']}" html-placeholder="Select Column"  html-autocomplete="on" />
                </div>
                <div class="fieldContainer">
                    <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['C1.1.1']}"/> <!-- Added C1.1.1 to the consolidated print -->
                </div>
                <div class="fieldLabelMargin" style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;font-weight:bold;">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<b>NDC # or Unique ID:</b>&nbsp; 
                    <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['C1.1.2']}" html-placeholder="Select Column"  html-autocomplete="on" />
                </div>
                <div class="fieldContainer">
                    <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['C1.1.2']}"/> <!-- Added C1.1.2 to the consolidated print -->
                </div>
                
                <div class="fieldLabelMargin" style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;font-weight:bold;">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<b>Manufacturer/Compounder:</b>&nbsp;
                    <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['C1.1.3']}" html-placeholder="Select Column"  html-autocomplete="on" />
                </div>
                <div class="fieldContainer">
                    <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['C1.1.3']}"/> <!-- Added C1.1.3 to the consolidated print -->
                </div>
                <div class="fieldLabelMargin" style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;font-weight:bold;">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<b>Lot #:</b>&nbsp; 
                    <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['C1.1.4']}" html-placeholder="Select Column"  html-autocomplete="on" />
                </div>
                <div class="fieldContainer">
                    <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['C1.1.4']}"/> <!-- Added C1.1.4 to the consolidated print -->
                </div>
                <div class="fieldLabelMargin" style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;font-weight:bold;">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<b>#2 Name and Strength:</b>&nbsp; 
                    <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['C1.2.1']}" html-placeholder="Select Column"  html-autocomplete="on" />
                </div>
                <div class="fieldContainer">
                    <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['C1.2.1']}"/> <!-- Added C1.2.1 to the consolidated print -->
                </div>
                
                <div class="fieldLabelMargin" style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;font-weight:bold;">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<b>NDC # or Unique ID:</b>&nbsp; 
                    <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['C1.2.2']}" html-placeholder="Select Column"  html-autocomplete="on" /> 
                </div>
                <div class="fieldContainer">   
                    <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['C1.2.2']}"/> <!-- Added C1.2.2 to the consolidated print -->
                </div>
                <div class="fieldLabelMargin" style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;font-weight:bold;">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<b>Manufacturer/Compounder:</b>&nbsp;  
                    <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['C1.2.3']}" html-placeholder="Select Column"  html-autocomplete="on" />
                </div>
                <div class="fieldContainer">
                    <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['C1.2.3']}"/> <!-- Added C1.2.3 to the consolidated print -->
                </div>
                <div class="fieldLabelMargin" style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;font-weight:bold;">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<b>Lot #:</b>&nbsp; 
                    <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['C1.2.4']}" html-placeholder="Select Column"  html-autocomplete="on" />
                </div>
                <div class="fieldContainer">
                    <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['C1.2.4']}"/> <!-- Added C1.2.4 to the consolidated print -->
                </div>
                
                <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;font-weight:bold;"><b>C2. Concom Med Prod and Therapy Dates:</b>&nbsp;
                    <apex:outputPanel > {!$Label.cmpl123cme__cmplt_c2_label} </apex:outputPanel> 
                </div><br/>
                
                <!-- Updated fields -->
                <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;font-weight:bold;"><b>C3. #1 Dose:</b>&nbsp; 
                    <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['C3.1.1.1']}" html-placeholder="Select Column"  html-autocomplete="on" />
                    <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['C3.1.1.1']}"/> <!-- Added C3.1.1.1 to the consolidated print --><!--Added missing angle bracket-->
                </div>
                <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;font-weight:bold;">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<b>Dose Units #1:</b>&nbsp; 
                    <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['C3.1.1.2']}" html-placeholder="Select Column"  html-autocomplete="on" />
                    <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['C3.1.1.2']}"/> <!-- Added C3.1.1.2 to the consolidated print -->
                </div>
                
                <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;font-weight:bold;">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<b>Frequency:</b>&nbsp; 
                    <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['C3.1.2.1']}" html-placeholder="Select Column"  html-autocomplete="on" />
                    <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['C3.1.2.1']}"/> <!-- Added C3.1.2.1 to the consolidated print -->
                </div>
                <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;font-weight:bold;">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<b>Frequency Units:</b>&nbsp; 
                    <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['C3.1.2.2']}" html-placeholder="Select Column"  html-autocomplete="on" />
                    <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['C3.1.2.2']}"/> <!-- Added C3.1.2.2 to the consolidated print -->
                </div>
                

                <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;font-weight:bold;">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<b>Route Used:</b>&nbsp; 
                    <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['C3.1.3.1']}" html-placeholder="Select Column"  html-autocomplete="on" />
                    <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['C3.1.3.1']}"/> <!-- Added C3.1.3.1 to the consolidated print -->
                </div>
                
                <div style="font-size:10px;font-family:'Arial Unicode MS',sans-serif;font-weight:bold;">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<b>#2 Dose:</b>&nbsp; 
                    <apex:inputText size="80" rendered="{!if(renderType != 'pdf',true,false)}" value="{!emdrReportMap['C3.2.1.1']}" html-placeholder="Select Column"  html-autocomplete="on" />
                    <apex:outputText rendered="{!if(renderType != 'pdf',false,true)}" value="{!ResultMap['C3.2.1.1']}"/> <!-- Added C3.2.1.1 to the consolidated print -->
                </div>
